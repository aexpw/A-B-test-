   "execution_count": 27,
   "id": "b8664fd8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Получение chi-square, p-value, degrees of freedom, expected frequencies\n",
    "chi2, p, dof, exp = sp.stats.chi2_contingency(cont_t_1)"
   ]
